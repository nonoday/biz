{
  "name": "juso-frontend-publish",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "vite --host 0.0.0.0 --port 4000",
    "build": "vite build",
    "build:ssr": "vite build --ssr src/entry-server.js",
    "preview": "vite preview",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs --fix --ignore-path .gitignore",
    "clear-vite-cache": "rmdir /s /q node_modules\\.vite\\",
    "serve:ssr": "node server.js",
    "serve:prod": "cross-env NODE_ENV=production node server.js",
    "start": "npm run build && npm run build:ssr && npm run serve:prod"
  },
  "dependencies": {
    "@ibsheet/loader": "^1.1.21",
    "@vue/server-renderer": "^3.5.13",
    "aos": "^2.3.4",
    "chart.js": "^4.4.2",
    "core-js": "^3.32.0",
    "express": "^4.21.2",
    "fabric": "^5.3.0",
    "ol": "^7.3.0",
    "pinia": "^2.1.4",
    "primeflex": "^3.3.1",
    "primeicons": "^6.0.1",
    "primevue": "^3.35.0",
    "proj4": "^2.9.0",
    "swiper": "^10.3.1",
    "v-calendar": "^3.1.2",
    "vue": "^3.3.4",
    "vue-router": "^4.2.4",
    "vue-server-renderer": "^2.7.16"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^4.2.3",
    "bootstrap": "^5.3.1",
    "bootstrap-icons": "^1.10.5",
    "cross-env": "^7.0.3",
    "eslint": "^8.45.0",
    "eslint-plugin-vue": "^9.15.1",
    "lodash": "^4.17.21",
    "sass": "^1.85.1",
    "terser": "^5.19.2",
    "vite": "^4.4.6",
    "vue3-clipboard": "^1.0.0"
  }
}
